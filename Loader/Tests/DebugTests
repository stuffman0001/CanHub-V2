local function loadSettings()
	if game:GetService("RunService"):IsStudio() then
		return "STUDIO"
	end

	if isfile("CanHub/CanHub.json") then
		local settingsData = readfile(SETTINGS_PATH)
		return HttpService:JSONDecode(settingsData)
	else
		return nil
	end
end

local RESULT = loadSettings()

if RESULT == nil then
loadstring(game:HttpGet("https://raw.githubusercontent.com/stuffman0001/CanHub-V2/refs/heads/main/Loader/Tests/Adonis%20Bypass.lua"))()
    return
end

if RESULT == "STUDIO" then
    return
end

if type(RESULT) == "table" then
    local safety = RESULT.safety
    if safety and safety.Bypassers == true then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/stuffman0001/CanHub-V2/refs/heads/main/Loader/Tests/Adonis%20Bypass.lua"))()
    end
end
